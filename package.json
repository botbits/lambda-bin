    "eslint": "^5.4.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.0.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
